<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HardwareStoreAdmin.AppClass.ImagenCompleta"
             Title="ImagenCompleta">
    <Grid>
        <Image x:Name="ImagenGrande"
               Aspect="AspectFit"
               HorizontalOptions="Center"
               VerticalOptions="Center"/>

        <Button Style="{StaticResource CloseIcon}"
                Clicked="CerrarImagenCompleta_Clicked"/>
    </Grid>
</ContentPage>